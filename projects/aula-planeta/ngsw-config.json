{
    "$schema": "../../node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html",
                    "/manifest.webmanifest",
                    "/*.css",
                    "/*.js",
                    "/assets/i18n/*.json",
                    "/assets/i18n/examples/*.json",
                    "/assets/logo_PLANETA200x200.png"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "lazy",
            "resources": {
                "files": [
                    "/assets/**.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)",
                    "/assets/icons/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)",
                    "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "analytics",
            "cacheConfig": {
                "maxSize": 100,
                "maxAge": "3d",
                "timeout": "5m",
                "strategy": "performance"
            },
            "urls": [
                "https://www.google-analytics.com/analytics.js"
            ]
        }
    ]
}