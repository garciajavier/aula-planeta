{
    "$schema": "../../node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html",
                    "/manifest.webmanifest",
                    "/*.css",
                    "/*.js",
                    "**/assets/i18n/*.json",
                    "**/assets/i18n/examples/*.json",
                    "**/assets/logo_PLANETA200x200.png",
                    "/libs/*.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "prefetch",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)",
                    "/assets/icons/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)",
                    "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "aula-api",
            "urls": [
                "/api/**",
                "https://lh3.googleusercontent.com/**"
            ],
            "cacheConfig": {
                "strategy": "performance",
                "maxSize": 1000,
                "maxAge": "1d"
            },
            "cacheQueryOptions": {
                "ignoreSearch": false
            }
        },
        {
            "name": "dynamicResources",
            "cacheConfig": {
                "maxSize": 1000,
                "maxAge": "180d",
                "timeout": "5m",
                "strategy": "performance"
            },
            "urls": [
                "**/images.unsplash.com/**",
                "**/source.unsplash.com/**",
                "**/cdnjs.cloudflare.com/**",
                "**/core-js-bundle/**",
                "https://fonts.googleapis.com/**",
                "https://fonts.gstatic.com/**"
            ]
        }
    ]
}